{
  "name": "just-dashboard",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/dashboard.js",
  "module": "src/index.js",
  "scripts": {
    "test": "mocha-webpack \"src/**/*.test.js\" --webpack-config webpack.test.config.js --require babel-polyfill",
    "test:watch": "mocha-webpack --watch \"src/**/*.test.js\" --webpack-config webpack.test.config.js  --require babel-polyfill",
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
    "html-coverage": "cross-env NODE_ENV=test nyc --reporter=html mocha-webpack \"src/**/*.test.js\" --webpack-config webpack.test.config.js",
    "generate-doc": "jsdoc -c jsdoc.conf.json --readme ./README.md",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm run test",
    "analyze": "webpack --profile --json > stats.json",
    "manual-test": "webpack-dev-server './src/manual_test.js' --hot --inline --output-filename 'manual_test.js'",
    "semantic-release": "semantic-release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "billboard.js": "^1.2.0",
    "d3": "^4.10.0",
    "jq-web": "^0.1.3",
    "sassline": "^2.1.2",
    "spinkit": "^1.2.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.9",
    "docdash": "^0.4.0",
    "eslint": "^4.16.0",
    "eslint-loader": "^1.9.0",
    "fake-style-loader": "^1.0.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "js-yaml": "^3.10.0",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.3.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "minami": "^1.2.3",
    "mocha": "^5.0.0",
    "mocha-webpack": "^1.0.1",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "sass-loader": "^6.0.6",
    "semantic-release": "^12.4.1",
    "should": "^13.2.1",
    "should-sinon": "0.0.6",
    "sinon": "^4.2.2",
    "style-loader": "^0.20.1",
    "stylish": "^1.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kantord/dashboard.git"
  }
}
